{
	"ConnectionStrings": {
		"Postgre": "Server=<server>;Port=<port>;Database=<database>;Username=<user>;Password=<password>"		
  },
  "TokenConfiguration" : {
    "Audience" : "AudienceName",
    "Issuer" : "IssuerName",
    "Secret" : "SecretKey",
    "Minutes" : 60,
    "DaysToExpire" : 1
  },
	"Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
